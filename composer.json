{
    "name": "yiisoft/yii-dev-tool",
    "type": "library",
    "description": "Yii development tool",
    "keywords": ["yii", "dev", "tool"],
    "homepage": "https://www.yiiframework.com/",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/yii-dev-tool/issues",
        "forum": "https://www.yiiframework.com/forum/",
        "wiki": "https://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/yii-dev-tool"
    },
    "require": {
        "php": "^8.0",
        "ext-curl": "*",
        "ext-openssl": "*",
        "knplabs/github-api": "^3.2",
        "nikic/php-parser": "^4.10",
        "nyholm/psr7": "^1.4.0",
        "php-http/guzzle7-adapter": "^1.0",
        "squizlabs/php_codesniffer": "^3.6",
        "symfony/console": "^6.0",
        "symfony/filesystem": "^6.0",
        "symfony/finder": "^6.0",
        "symfony/polyfill-php73": "^1.22",
        "symfony/process": "^6.0",
        "symfony/yaml": "^6.0",
        "symplify/git-wrapper": "^10.2.0",
        "yiisoft/files": "^2.0",
        "yiisoft/var-dumper": "^1.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "rector/rector": "^0.19.0"
    },
    "autoload": {
        "psr-4": {
            "Yiisoft\\YiiDevTool\\": "src"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yiisoft\\YiiDevTool\\Test\\": "tests"
        }
    }
}
